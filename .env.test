# Test Environment Configuration for DreamLake Python SDK
# Used when running pytest with remote tests

# DreamLake Server URL (when running remote tests)
DREAMLAKE_SERVER_URL=http://localhost:3000

# Test credentials
DREAMLAKE_TEST_USER=test-user
DREAMLAKE_API_KEY=  # Auto-generated from username via JWT

# S3 Configuration (optional, for direct S3 testing)
AWS_ENDPOINT=http://localhost:9002
AWS_ACCESS_KEY_ID=minioadmin
AWS_SECRET_ACCESS_KEY=minioadmin
S3_BUCKET=dreamlake-test
